{
    "name": "realodix/change-case",
    "description": "Convert strings between camelCase, PascalCase, Capital Case, snake_case and more.",
    "license": "MIT",
    "keywords": ["lowercase", "uppercase", "camel-case", "snake-case", "pascal-case", "change-case", "constant-case"],
    "authors": [{"name": "Budi Hermawan", "role": "Developer" }],
    "homepage": "https://github.com/realodix/change-case",
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "realodix/relax": "dev-main"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Realodix\\ChangeCase\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Realodix\\ChangeCase\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "3.x-dev"
        }
    }
}
